{"common": {
    "fdcfg": "@PREFIX@/freeDiameter/hss_rel14_fd.conf",
    "originhost": "@HSS_FQDN@",
    "originrealm": "@REALM@" 
 },
 "hss": {
    "gtwhost": "*",
    "gtwport" : 9080,
    "restport" : 9081,
    "ossport" : 9082,
    "casssrv": "@cassandra_Server_IP@", 
    "cassusr": "root",
    "casspwd": "root",
    "cassdb" : "vhss",
    "casscoreconnections" : 2,
    "cassmaxconnections" : 8,
    "cassioqueuesize" : 32768,
    "cassiothreads" : 2,    
    "randv"  : true,
    "optkey" : "@OP_KEY@",
    "reloadkey"  : false,
    "roamallow"  : @ROAMING_ALLOWED@,
    "logsize": 20,
    "lognumber": 5,
    "logname": "/tmp/hss.log",
    "logqsize": 8192,
    "statlogsize": 20,
    "statlognumber": 5,
    "statlogname": "/tmp/hss_stat.log",
    "auditlogsize": 20,
    "auditlognumber": 5,
    "auditlogname": "/tmp/hss_audit.log",
    "statfreq": 2000,
    "numworkers": 4,
    "concurrent": 10,
    "ossfile": "conf/oss.json"    
 }
}
