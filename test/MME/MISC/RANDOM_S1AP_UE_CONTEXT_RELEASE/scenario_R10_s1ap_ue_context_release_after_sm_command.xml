  <set_var name="AUTN"                              value="$ITTI_NAS_DOWNLINK_DATA_REQ.NAS.AUTHENTICATION_REQUEST.AUTHENTICATION_PARAMETER_AUTN"   />
  <!-- variable AUTHENTICATION_RESPONSE_PARAMETER is computed and set by scenario player now -->
